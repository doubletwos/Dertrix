
@{
	ViewBag.Title = "SchCalendar";
}

<div class="jumbotron" style="height:100vh;">
	<br />
	<br />
	@Html.Partial("_WidgetDashboard1")

	<hr />

	<div class="container">
		<div class="row">
			<div class="col-lg-6">

				<h3 style="font-family :'Exo 2', sans-serif;">
					Events
					<i class="fa fa-black-tie fa-lg" style="color:green;" aria-hidden="true"></i>
				</h3>

				@{
					Html.RenderAction("EventDisplayPanel", "OrgEvents");
				}

			</div>
			<div class="col-lg-6">
				<h3 style="font-family :'Exo 2', sans-serif;">
					Diary Dates
					<i class="fa fa-calendar fa-lg" style="color:blue;" aria-hidden="true"></i>
				</h3>
				@{
					Html.RenderAction("ImpDateDisplayPanel", "OrgImportantDates");
				}
	



			</div>
		</div>
	</div>
</div>


